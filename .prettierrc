{
  "plugins": ["prettier-plugin-tailwindcss"],
  "trailingComma": "es5",
  "bracketSpacing": true,
  "endOfLine": "lf",
  "htmlWhitespaceSensitivity": "css",
  "jsxBracketSameLine": false,
  "quoteProps": "as-needed",
  "proseWrap": "preserve",
  "requirePragma": false,
  "arrowParens": "avoid",
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "printWidth": 100
}
